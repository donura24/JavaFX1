<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.WebView?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="WebVieww.Controller">
   <children>
      <WebView fx:id="myWebView" layoutY="61.0" prefHeight="341.0" prefWidth="600.0" />
      <TextField fx:id="myTextField" layoutX="-1.0" layoutY="8.0" onAction="#loadPage" prefHeight="24.0" prefWidth="600.0" />
      <Button fx:id="myLoadButton" layoutX="14.0" layoutY="32.0" mnemonicParsing="false" onAction="#loadPage" text="Load" />
      <Button fx:id="myRefreshButton" layoutX="70.0" layoutY="32.0" mnemonicParsing="false" onAction="#refreshPage" text="Refresh" />
      <Button fx:id="myZoomInButton" layoutX="144.0" layoutY="32.0" mnemonicParsing="false" onAction="#zoomIn" text="Zoom +" />
      <Button fx:id="myZoomOutButton" layoutX="224.0" layoutY="32.0" mnemonicParsing="false" onAction="#zoomOut" text="Zoom -" />
      <Button fx:id="myPageHistoryButton" layoutX="299.0" layoutY="32.0" mnemonicParsing="false" onAction="#displayHistory" text="History" />
      <Button fx:id="myBackButton" layoutX="372.0" layoutY="32.0" mnemonicParsing="false" onAction="#back" text="Back" />
      <Button fx:id="myForwardButton" layoutX="430.0" layoutY="32.0" mnemonicParsing="false" onAction="#forward" text="Forward" />
   </children>
</AnchorPane>
