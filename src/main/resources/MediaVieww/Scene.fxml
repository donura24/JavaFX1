<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MediaVieww.Controller">
   <children>
      <MediaView fx:id="myMediaView" fitHeight="300.0" fitWidth="300.0" layoutX="150.0" layoutY="14.0" />
      <Button fx:id="myPlayButton" layoutX="88.0" layoutY="339.0" mnemonicParsing="false" onAction="#playMedia" text="Play" />
      <Button fx:id="myPauseButton" layoutX="269.0" layoutY="339.0" mnemonicParsing="false" onAction="#pauseMedia" text="Pause" />
      <Button fx:id="myResetButton" layoutX="450.0" layoutY="339.0" mnemonicParsing="false" onAction="#resetMedia" text="Reset" />
   </children>
</AnchorPane>
